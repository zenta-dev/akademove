NODE_ENV=development
RUNTIME=workerd

# Cloudflare (required for production deployment)
ZONE_ID=
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_API_TOKEN=

ALCHEMY_PASSWORD=

ROOT_DOMAIN=localhost
SERVER_URL=http://localhost:3000
WEB_URL=http://localhost:5173

AUTH_SECRET=
AUTH_URL=

# Database (PostgreSQL)
POSTGRES_USER=akademove_user
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=akademove_db
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@localhost:5432/${POSTGRES_DB}

# S3/MinIO (for file uploads)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
S3_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=${MINIO_ROOT_USER}
S3_SECRET_ACCESS_KEY=${MINIO_ROOT_PASSWORD}
S3_PUBLIC_URL=http://localhost:9000

# Google Maps API (required for location services)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Email Service (Resend)
RESEND_API_KEY=re_your_resend_api_key

# Payment Gateway (Midtrans)
MIDTRANS_IS_PRODUCTION=false
MIDTRANS_SERVER_KEY=your_midtrans_server_key
MIDTRANS_CLIENT_KEY=your_midtrans_client_key
MIDTRANS_EXPERIMENTAL_IRIS=false

# Firebase Configuration (required for authentication and push notifications)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_PROJECT_ID=your-project-id
FIREBASE_STORAGE_BUCKET=your-project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX
FIREBASE_VAPID_KEY=your_vapid_key

# Firebase Service Account (JSON string for server-side operations)
FIREBASE_SERVICE_ACCOUNT={"type":"service_account","project_id":"your-project-id","private_key_id":"...","private_key":"...","client_email":"...","client_id":"...","auth_uri":"...","token_uri":"...","auth_provider_x509_cert_url":"...","client_x509_cert_url":"..."}